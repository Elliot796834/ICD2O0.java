//Circumfrence code

import java.lang.Math; // import the math library

public class Circle {
    public static void main(String[] args) {
        double circ;
        double rad = 0.5; // the radius
        circ = 2 * Math.PI * rad; // the circumference

        System.out.println("The circumference of a circle of radius " + rad + " is " + circ);
    }
}
